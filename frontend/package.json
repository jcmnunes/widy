    "@types/styled-components": "^5.1.0",
    "@types/yup": "^0.26.37",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-plugin-macros": "^2.8.0",
    "docz": "^2.3.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
